  <gazebo>
    <plugin name="rotor_0_blade_1" filename="libLiftDragPlugin.so">
      <!-- <a0>0.1</a0> -->
      <a0>0.16</a0>               <!-- lift-curve slope -->

      <!-- <alpha_stall>1.4</alpha_stall> -->
      <alpha_stall>1.2</alpha_stall>    <!-- stall angle, ~70° in radians -->

      <!-- <cla>0.2500</cla> -->
      <cla>0.36</cla>            <!-- lift coefficient per rad -->

      <!-- <cda>0.10</cda> -->
      <cda>0.16</cda>            <!-- drag coefficient per rad -->

      <!-- <cma>0.00</cma> -->
      <cma>0.00</cma>            <!-- moment coefficient -->

      <!-- <cla_stall>-0.025</cla_stall> -->
      <cla_stall>-0.06</cla_stall>   <!-- lift coef in stall -->

      <!-- <cda_stall>0.0</cda_stall> -->
      <cda_stall>0.06</cda_stall>    <!-- drag coef in stall -->

      <!-- <cma_stall>0.0</cma_stall> -->
      <cma_stall>0.00</cma_stall>    <!-- moment coef in stall -->

      <!-- <area>0.2</area> -->
      <area>0.245</area>             <!-- Prop disk area = π·(0.2794 m)² ≈ 0.245 m² -->

      <air_density>1.2041</air_density>

      <!-- <cp>0.084 0 0</cp> -->
      <cp>0.18 0 0</cp>              <!-- Center-of-pressure ≈ 0.65·radius → 0.18 m out on the blade -->
      
      <forward>0 1 0</forward>
      <upward>0 0 1</upward>
      <link_name>final_iris::rotor_0</link_name>
    </plugin>
