<!-- URDF IRIS USING VELODYNE -->

<?xml version="1.0" ?>
<robot name="final_iris" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Dummy link -->
  <link name="dummy_link"/>

  <!-- Fixed joint between dummy_link and base_link 0.195-->
  <joint name="fixed_joint" type="fixed">
    <parent link="dummy_link"/>
    <child link="base_link"/>
    <origin xyz="0 0 0.195" rpy="0 0 0"/>
    <!-- <origin xyz="0 0 0.05" rpy="0 0 0"/> -->
  </joint>



  <link name="base_link">
    <inertial>
      <mass value="1.5"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.008" ixy="0" ixz="0" iyy="0.015" iyz="0" izz="0.017"/>
    </inertial>
    <collision name="base_link">
      <origin rpy="0 0 0" xyz="0      0      -0.08"/>
      <geometry>
        <box size="0.47 0.47 0.23"/>
      </geometry>
    </collision>
    <visual>
      <geometry>
        <mesh filename="package://iq_sim/models/iris_base/meshes/iris.dae" scale="1.0 1.0 1.0"/>
      </geometry>
    </visual>
    <visual>
      <origin rpy="0 0 0" xyz="0.123   0.22    -0.11"/>
      <geometry>
        <cylinder length="0.17" radius="0.005"/>
      </geometry>
    </visual>
    <visual>
      <origin rpy="0 0 0" xyz="0.123   -0.22     -0.11"/>
      <geometry>
        <cylinder length="0.17" radius="0.005"/>
      </geometry>
    </visual>
    <visual>
      <origin rpy="0 0 0" xyz="-0.14     0.21     -0.11"/>
      <geometry>
        <cylinder length="0.17" radius="0.005"/>
      </geometry>
    </visual>
    <visual>
      <origin rpy="0 0 0" xyz="-0.14    -0.21     -0.11"/>
      <geometry>
        <cylinder length="0.17" radius="0.005"/>
      </geometry>
    </visual>
  </link>
 
<!-- Velodyne joint -->
<joint name="velodyne_joint" type="fixed">
  <parent link="base_link"/>
  <child link="velodyne_link"/>
  <!-- <origin xyz="0 0 0.1" rpy="0 0 -1.5708"/> -->
  <origin xyz="0 0 0.1" rpy="0 0 0"/>
</joint>

<!-- Velodyne physical link-->
<link name="velodyne_link">
  <inertial>
    <mass value="0.5"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
  </inertial>
  <visual>
    <geometry>
      <mesh filename="package://velodyne_description/meshes/HDL32E_base.dae"/>
    </geometry>
  </visual>
</link> 


<!-- Velodyne sensor definition -->
<gazebo reference="velodyne_link">
  <sensor type="ray" name="velodyne_sensor">
    <pose>0 0 0 0 0 0</pose>
    <update_rate>20.0</update_rate>
    <always_on>true</always_on>
    <visualize>false</visualize> 
    <ray>
      <scan>
        <horizontal>
          <samples>180</samples>
          <min_angle>-3.14</min_angle>
          <max_angle>3.14</max_angle>
        </horizontal>
        <vertical>
          <samples>64</samples>
          <min_angle>-0.122173</min_angle>
          <max_angle>0.907571</max_angle>
        </vertical>
      </scan>
      <range>
        <min>0.5</min>
        <max>40.0</max>
        <resolution>0.01</resolution>
      </range>
    </ray>
    <plugin name="velodyne_plugin" filename="libgazebo_ros_velodyne_laser.so">
  <topicName>/velodyne_points</topicName>
  <frameName>velodyne_link</frameName>
  <robotNamespace>/</robotNamespace>
</plugin>

  </sensor>
</gazebo>



