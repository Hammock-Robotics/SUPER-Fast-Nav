      <inertia ixx="9.75e-06" ixy="0" ixz="0" iyy="0.000166704" iyz="0" izz="0.000167604"/>
    </inertial>
    <collision>
      <geometry>
        <cylinder radius="0.1" length="0.005"/>
      </geometry>
    </collision>
    <visual>
      <geometry>
        <mesh filename="package://iq_sim/models/iris_base/meshes/iris_prop_ccw.dae"/>
      </geometry>
      <material name="blue">
        <script>
          <name>Gazebo/Blue</name>
        </script>
      </material>
    </visual>
  </link>

  <joint name="rotor_0_joint" type="revolute">
