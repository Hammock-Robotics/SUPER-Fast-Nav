<?xml version="1.0"?>
<robot name="iris" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <link name="base_footprint"/>

  <!-- Fixed joint between base_footprint and base_link -->
  <joint name="base_joint" type="fixed">
      <origin xyz="0.0 0.0 0.135" rpy="0.0 0.0 0.0"/>
      <parent link="base_footprint"/>
      <child link="base_link"/>
      <axis xyz="0.0 0.0 0.0"/>
        
  </joint>


  <link name="base_link">
    <inertial>
      <mass value="1.5"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="0.008" ixy="0" ixz="0" iyy="0.015" iyz="0" izz="0.017"/>
    </inertial>
    <collision name="base_link_collision_fl">
      <origin xyz="0.13 0.22 -0.08" rpy="0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.18"/>
      </geometry>
    </collision>
    <collision name="base_link_collision_fr">
      <origin xyz="0.13 -0.22 -0.08" rpy="0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.18"/>
      </geometry>
    </collision>
    <collision name="base_link_collision_br">
      <origin xyz="-0.13 -0.22 -0.08" rpy="0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.18"/>
      </geometry>
    </collision>
    <collision name="base_link_collision_bl">
      <origin xyz="-0.13 0.22 -0.08" rpy="0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.18"/>
      </geometry>
    </collision>
    <visual name="base_link_visual">
      <geometry>
        <mesh filename="package://iq_sim/models/iris_base/meshes/iris.dae"/>
      </geometry>
      <material name="dark_grey">
        <script>
          <name>Gazebo/DarkGrey</name>
        </script>
      </material>
    </visual>
    <visual name="front_left_leg_visual">
      <origin xyz="0.123 0.22 -0.11" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.005" length="0.17"/>
      </geometry>
      <material name="flat_black">
        <script>
          <name>Gazebo/FlatBlack</name>
          <uri>file://media/materials/scripts/gazebo.material</uri>
        </script>
      </material>
    </visual>
    <visual name="front_right_leg_visual">
      <origin xyz="0.123 -0.22 -0.11" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.005" length="0.17"/>
      </geometry>
      <material name="flat_black">
        <script>
          <name>Gazebo/FlatBlack</name>
          <uri>file://media/materials/scripts/gazebo.material</uri>
        </script>
      </material>
    </visual>
    <visual name="rear_left_leg_visual">
      <origin xyz="-0.140 0.21 -0.11" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.005" length="0.17"/>
      </geometry>
      <material name="flat_black">
        <script>
          <name>Gazebo/FlatBlack</name>
          <uri>file://media/materials/scripts/gazebo.material</uri>
        </script>
      </material>
    </visual>
    <visual name="rear_right_leg_visual">
      <origin xyz="-0.140 -0.21 -0.11" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.005" length="0.17"/>
      </geometry>
      <material name="flat_black">
        <script>
          <name>Gazebo/FlatBlack</name>
          <uri>file://media/materials/scripts/gazebo.material</uri>
        </script>
      </material>
    </visual>
  </link>

  <link name="iris/ground_truth/odometry_sensorgt_link">
    <inertial>
      <mass value="0.15"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0002" iyz="0" izz="0.0002"/>
    </inertial>
  </link>

  <joint name="iris_ground_truth_odometry_sensorgt_joint" type="revolute">
    <parent link="base_link"/>
    <child link="iris/ground_truth/odometry_sensorgt_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="0" upper="0" effort="1000" velocity="1"/>
    <dynamics damping="1.0"/>
  </joint>

  <link name="iris/imu_link">
    <inertial>
      <mass value="0.15"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="0.00001" ixy="0" ixz="0" iyy="0.00002" iyz="0" izz="0.00002"/>
    </inertial>
    <sensor name="imu_sensor" type="imu">
      <origin xyz="0 0 0" rpy="3.141593 0 0"/>
      <always_on>1</always_on>
      <update_rate>1000.0</update_rate>
    </sensor>
  </link>

  <joint name="iris_imu_joint" type="revolute">
    <parent link="base_link"/>
    <child link="iris/imu_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="0" upper="0" effort="1000" velocity="1"/>
    <dynamics damping="1.0"/>
  </joint>

