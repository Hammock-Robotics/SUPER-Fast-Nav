<launch>
  <!-- Declare an argument for the YAML config -->
  <arg name="config_file" default="$(find arducam)/config/1280x720.yaml" />

  <!-- USB camera node -->
  <node name="camera" pkg="arducam" type="usb_cam_node" output="screen">
    <rosparam file="$(arg config_file)" command="load"/>
  </node>

  <!-- RViz node -->
  <arg name="rviz_config" default="$(find arducam)/rviz/camera_view.rviz" />
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(arg rviz_config)" required="true" />
</launch>
